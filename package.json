  "author": "Noah Brenner",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest --verbose",
    "lint:lint": "eslint --ignore-path .gitignore .",
    "lint:types": "tsc --noEmit",
